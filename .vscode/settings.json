    "files.associations": {
        "*.embeddedhtml": "html",
        "qjsonobject": "cpp",
        "qnetworkaccessmanager": "cpp",
        "qguiapplication": "cpp",
        "array": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "vector": "cpp",
        "qnetworkrequest": "cpp",
        "qurlquery": "cpp",
        "qjsonarray": "cpp",
        "qscopedpointer": "cpp",
        "qqmlcontext": "cpp",
        "istream": "cpp",
        "locale": "cpp",
        "tuple": "cpp",
        "iosfwd": "cpp",
